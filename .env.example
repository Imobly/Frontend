# Frontend Environment Variables - Imobly

# URL do backend principal
NEXT_PUBLIC_API_URL=http://localhost:8000/api/v1

# URL da API de autenticação
NEXT_PUBLIC_AUTH_API_URL=http://localhost:8001

# URL da aplicação frontend
NEXT_PUBLIC_APP_URL=http://localhost:3000


NEXT_PUBLIC_APP_NAME=Imobly
NEXT_PUBLIC_APP_VERSION=1.0.0

# Environment (development | production | test)
NODE_ENV=production


# Desabilitar telemetria do Next.js
NEXT_TELEMETRY_DISABLED=1

# Estas variáveis são usadas apenas no ambiente Docker
